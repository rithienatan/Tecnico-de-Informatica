algoritmo "exercicio 2"

var
kg,h,imc:real
X: inteiro
inicio
   repita
      escreval ("Boa tarde!! ")
      escreval ("insira o peso")
      leia(kg)
      escreval ("insira a altura")
      leia(h)
      imc<-kg/h^2
      escreval ("indice de massa corporal",imc)
      X<-X+1
   ate (X=5)

fimalgoritmo
