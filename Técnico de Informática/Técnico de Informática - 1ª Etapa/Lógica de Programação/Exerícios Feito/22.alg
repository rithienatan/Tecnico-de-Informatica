algoritmo "exercicio 4"

var
N, AN, IA, IM, ID:inteiro
inicio
escreval("ano de nacimento")
leia(N)
escreval("ano atual")
leia(AN)
  IA<-AN-N
escreval("idade em anos",IA)
  IM<-IM*12
escreval("idade em meses",IM)
  ID<-IM*30
escreval("idade em dias",ID)
// Seção de Comandos 
fimalgoritmo
